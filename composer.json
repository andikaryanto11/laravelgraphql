{
    "name": "andikaryanto11/laravelgraphql",
    "description": "Graphql package for Laravel",
    "require-dev": {
        "squizlabs/php_codesniffer": "4.0.x-dev"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "LaravelGraphQL\\": "src/"
        }
    },
    "authors": [
        {
            "name": "andik aryanto"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "webonyx/graphql-php": "15.7.0"
    },
    "extra": {
        "laravel": {
            "dont-discover": [],
            "providers": [
                "LaravelGraphQL\\App\\Providers\\GraphQLProvider"
            ],
            "aliases": {
                "GraphQL": "LaravelGraphQL\\System\\Support\\Facades\\GraphQL"
            }
        }
    },
    "repositories": [
        {
            "url": "https://github.com/andikaryanto11/laravelcommon.git",
            "type": "git"
        }
    ]
}
